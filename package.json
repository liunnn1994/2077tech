{
  "name": "07-data",
  "version": "0.0.1",
  "description": "领起云享",
  "author": "Norris Lau",
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "preinstall": "npx only-allow pnpm",
    "format:pkg": "prettier --ignore-unknown --write package.json",
    "upgrade:deps": "npx taze -r -w",
    "upgrade:engines": "node scripts/upgradeEngines.js",
    "upgrade:all": "run-s upgrade:deps upgrade:engines format:pkg",
    "dev": "turbo generate-web-app-json dev",
    "check-types": "turbo generate-web-app-json check-types",
    "format": "prettier --ignore-unknown --write",
    "lint:fix": "turbo generate-web-app-json lint:fix",
    "semantic-release": "semantic-release",
    "simple-git-hooks": "simple-git-hooks",
    "cz": "git-cz"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint -e"
  },
  "dependencies": {
    "axios": "^1.7.8",
    "date-fns": "^3.6.0",
    "lodash-es": "^4.17.21",
    "turbo": "^2.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.1",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-oxlint": "^0.9.10",
    "git-cz": "^4.9.0",
    "gray-matter": "^4.0.3",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^7.0.1",
    "oxlint": "^0.14.0",
    "prettier": "^3.4.1",
    "prettier-plugin-packagejson": "^2.5.6",
    "semantic-release": "^24.1.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": "22.11.0"
  },
  "volta": {
    "node": "22.11.0",
    "pnpm": "9.14.4"
  }
